<web-app>
	<servlet>
		<servlet-name>shubham</servlet-name>
		<jsp-file>/welcome.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>shubham</servlet-name>
		<url-pattern>/welcome</url-pattern>
	</servlet-mapping>
	<context-param>
